#include <stdio.h>
int main()
{
	int i;
	int ary[9]; //9개의 숫자 입력
	int max = 0;
	int j =0 ;

	for (i = 1; i < 10; i++)
	{
		scanf_s("%d", &ary[i]); //숫자 입력

		if (max < ary[i]) 
		{
			max = ary[i]; // max값보다 크면 max이 됨
			j = i;  //max값의 순서를 i변수를 이용하여 저장
		}
		
	}

	printf("%d\n",max);
	printf("%d",j);

	return 0;
}
